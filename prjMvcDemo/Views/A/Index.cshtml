
@{
    ViewBag.Title = "Index";
    string photo = "anonymous.png";
    if (Model != null)
    {
        photo = Model.fid + ".png";
    }

}

<h2>客戶資料查詢</h2>

<p>
    <img src="~/images/@photo" width="100" height="100" />
    @{
        if (Model != null)
        {
            <h5>@Model.fname</h5><br/>
            @:電話:@Model.fname/@Model.fEmail

        }else
            {
            <h3>查無資料</h3>
            }
    }

</p>

